<div class="container">
<div style="display: none;" id="user" data-user= <%= current_user.id %>></div>
  <div class="row">
    <%= link_to "Start A New Project", root_path %>
    <div class="col-md-4">
      <h2>Open Projects</h2>
      <ul class="list-group open-project">
        <li class="list-group-item">
        </li>
      </ul>
    </div>

    <div class="col-md-4">
      <h2>Accepted Projects</h2>
      <div style="display: none;" id="token" data-token= <%= @token %>></div>
      <ul class="list-group accepted-project">
      </ul>
    </div>

    <div class="col-md-4">
      <h2>Completed Projects</h2>
      <ul class="list-group closed-project">
        <li class="list-group-item">
        </li>
      </ul>
    </div>
  </div>
</div>

<%= javascript_include_tag "user_dashboard.js" %>